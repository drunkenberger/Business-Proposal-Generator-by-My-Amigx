{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/proposals/generate.ts": {
      "maxDuration": 30
    },
    "api/proposals/generate-fast.js": {
      "maxDuration": 15
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}